                  <StackPane layoutX="31.0" layoutY="-69.0" prefHeight="427.0" prefWidth="649.0">
                     <children>
                        <ImageView fitHeight="398.0" fitWidth="641.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@banner.jpg" />
                           </image>
                        </ImageView>
                        <AnchorPane prefHeight="392.0" prefWidth="606.0">
                           <children>
                              <StackPane layoutX="-96.0" layoutY="-20.0" prefHeight="150.0" prefWidth="200.0" />
                              <ImageView fitHeight="239.0" fitWidth="176.0" layoutX="125.0" layoutY="61.0" pickOnBounds="true" preserveRatio="true">
