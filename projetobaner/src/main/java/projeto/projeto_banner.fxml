                  <children>
                      <ImageView fx:id="imageView" fitWidth="2.0" />
                  </children>
              </StackPane>
              <Button fx:id="selectImagesButton" layoutX="309.0" layoutY="486.0" mnemonicParsing="false" onAction="#selectImages" prefHeight="27.0" prefWidth="182.0" text="Selecionar imagens">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
            <AnchorPane layoutX="60.0" layoutY="61.0" prefHeight="384.0" prefWidth="679.0">
